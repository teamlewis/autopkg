		<key>DOWNLOAD_URL</key>
		<string>https://download.expressvpn.xyz/clients/mac/expressvpn_mac_7.9.7.8_release.pkg</string>
		<key>NAME</key>
		<string>ExpressVPN</string>
	</dict>
	<key>MinimumVersion</key>
	<string>1.0.0</string>
	<key>Process</key>
	<array>
		<dict>
